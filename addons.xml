<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.proba" name="proba repository" version="1.0.0" provider-name="Maniac">
	<extension point="xbmc.addon.repository" name="Maniac Repository">
		<info compressed="false">http://raw.github.com/Carlesto/repository-proba/master/addons.xml</info>
		<checksum>http://raw.github.com/Carlesto/repository-proba/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/Carlesto/repository-proba/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Maniac Addon</summary>
		<description>Repositorio NO oficial para Kodi</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>

<addon id="script.module.six"
       name="six"
       version="1.14.0+matrix.2"
       provider-name="gutworth">
  <requires>
    <import addon="xbmc.python"	version="3.0.0"/>
  </requires>
  <extension point="xbmc.python.module"
             library="lib" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en_GB">Python 2 and 3 compatibility utilities.</summary>
    <description lang="en_GB">Six is a Python 2 and 3 compatibility library. It provides utility functions for smoothing over the differences between the Python versions with the goal of writing Python code that is compatible on both Python versions. See the documentation for more information on what is provided.</description>
    <platform>all</platform>
    <license>MIT</license>
    <source>https://pypi.org/project/six/</source>
    <website>https://pypi.org/project/six/</website>
    <assets>
      <icon>icon.png</icon>
    </assets>
  </extension>
</addon>
</addons>
